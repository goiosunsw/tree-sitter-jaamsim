{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "jaamsim",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "rec_decl"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "sentence"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "junk": {
      "type": "PATTERN",
      "value": "[^%@\\s\\n\\t][^%@]*"
    },
    "rec_decl": {
      "type": "STRING",
      "value": "RecordEdits"
    },
    "sentence": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_subj"
        },
        {
          "type": "SYMBOL",
          "name": "property"
        },
        {
          "type": "SYMBOL",
          "name": "_val_exp"
        }
      ]
    },
    "_subj": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "define"
        },
        {
          "type": "SYMBOL",
          "name": "object"
        }
      ]
    },
    "define": {
      "type": "STRING",
      "value": "Define"
    },
    "object": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_-]*"
    },
    "this": {
      "type": "STRING",
      "value": "this"
    },
    "property": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_]*"
    },
    "_val_exp": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "value"
            }
          },
          {
            "type": "STRING",
            "value": "}"
          }
        ]
      }
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "bool"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "object"
        },
        {
          "type": "SYMBOL",
          "name": "entity_attribute"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "path"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9.-]+"
    },
    "bool": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "TRUE"
        },
        {
          "type": "STRING",
          "value": "FALSE"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\\'[^']*\\'"
    },
    "entity_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_obj_or_this"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "property"
        }
      ]
    },
    "_obj_or_this": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_obj_delim"
        },
        {
          "type": "SYMBOL",
          "name": "this"
        }
      ]
    },
    "_obj_delim": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "object"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "value"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "path": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9._/-]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[\\s\\n\\t]"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}