{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jaamsim",
      "camelcase": "JaamSim",
      "title": "Jaamsim",
      "scope": "source.cfg",
      "file-types": [
        "cfg","bas","inc"
      ],
      "injection-regex": "^jaamsim$",
      "class-name": "TreeSitterJaamsim",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Language grammar for jaamsim configuration files",
    "authors": [
      {
        "name": "Andre Almeida",
        "email": "andre.almeida@hypercubescientific.com.au"
      }
    ],
    "links": {
      "repository": "https://github.com/goiosunsw/tree-sitter-jaamsim"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
